{
  "name": "pdfmake-browserified-0.1.18",
  "version": "0.1.0",
  "description": "Browserified version of pdfmake 0.1.18, font Roboto included.",
  "main": "./build/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/xErik/pdfmake-browserified"
  },
  "scripts": {
    "build": "browserify src/pdfmake.js -g aliasify -s createPdf -p browserify-derequire -d -p [minifyify --no-map] -o build/bundle.js",
    "prestart": "npm run build && cp ./build/bundle.js ./test/",
    "start": "node server.js"
  },
  "author": "xErik",
  "license": "MIT",
  "dependencies": {},
  "keywords": [
    "pdfmake",
    "browserify",
    "pdfkit",
    "pdf",
    "browser"
  ],
  "devDependencies": {
    "aliasify": "^1.7.2",
    "browserify-derequire": "^0.9.4",
    "connect": "^3.3.5",
    "minifyify": "^7.0.1",
    "open": "0.0.5",
    "serve-static": "^1.9.3",
    "brfs": "1.0.2",
    "pdfmake": "0.1.18"
  },
  "browserify": {
    "transform": [
      "aliasify"
    ]
  },
  "aliasify": {
    "aliases": {
      "fs": "./src/virtual-fs.js"
    }
  }
}
